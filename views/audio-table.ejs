<div class="audio-table-container">

    <table class="audio-table">
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>Creator</th>
            <th>Length</th>
        </tr>
        <% let i = 1; %>
        <% for (let id in tableRows) { %>
            <tr class="audio-row" id="<%= id %>">
                <td class="row-num"> <%= i++ %>
                </td>
                <td class="title"> <%= tableRows[id].title %> </td>
                <td class="creator"> <%= tableRows[id].creator %> </td>
                <td class="length"> <%= tableRows[id].length %> </td>
            </tr>
        <% } %>
    </table>
</div>
<script src="javascripts/audio-table.js"></script>